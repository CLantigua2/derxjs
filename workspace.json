{
  "version": 2,
  "projects": {
    "examples-angular-list": "apps/examples/angular/list",
    "examples-angular-list-e2e": "apps/examples/angular/list-e2e",
    "examples-list-view-model-implementation": "libs/examples/list-view-model-implementation",
    "reducer": "packages/reducer",
    "view-model": "packages/view-model"
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    }
  }
}
